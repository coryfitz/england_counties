{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react-jsx"
    },
    "include": [
        "src/Config.tsx",
        "src/Home.tsx",
        "src/index.tsx",
        "src/Quiz.tsx",
        "src/QuizContext.tsx",
        "src/Study.tsx",
        "src/StudyContext.tsx"
    ]
  }
  